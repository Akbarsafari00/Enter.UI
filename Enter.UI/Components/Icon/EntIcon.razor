@inherits EntBaseComponent
@namespace Enter.UI.Components


@if (Type == EntIconType.IconTag)
{
    <i style="@Style" @attributes="Attributes" class="@RootCss @Data"></i>
}else if (Type == EntIconType.ImageTag)
{
    <img style="@Style" @attributes="Attributes" class="@RootCss" src="@Data" />

}else if(Type == EntIconType.SvgContent)
{
    <svg style="@Style" @attributes="Attributes" class="@RootCss">
        @Data
    </svg>
}







